project: test
environments:
- name: environment1
  services:
  - name: service1_environment1
    namespace: environment-stg
    volumes:
    - name: vol1
      size: 1G
      path: /tmp/asd
- name: environment2
  namespace: environment-dev
  services:
  - name: service1_environment2
    external_url: www.test.com
    volumes:
    - name: vol1
      size: 1G
      path: /tmp/asd
      modes: ReadWriteMany
    hpa:
      min_replicas: 2
      max_replicas: 3
      target_cpu_utilization_percentage: 50
  - name: service2_environment2
  - name: testdb
    type: "mysql"
    version: "5.6"
