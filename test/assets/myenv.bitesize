project: myproject
environments:
- name: myenv
  namespace: environment-dev
  services:
  - name: testdb
    type: "neptune"
    version: "1.0.1.0.200264.0"
    options:
      ApplyImmediately: True
      db_instances:
        - db_name: "db01"
          db_instance_class: "db.r4.2xlarge"
        - db_name: "db02"
          db_instance_class: "db.r4.xlarge"
